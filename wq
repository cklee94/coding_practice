[32m+[m[32mclass Sol:[m[41m[m
[32m+[m[32m    def __init__(self):[m[41m[m
[32m+[m[32m        self.N = int(input())[m[41m[m
[32m+[m[32m        self.request = list(map(int, input().split()))[m[41m[m
[32m+[m[32m        self.M = int(input())[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        self.request.sort(reverse=True)[m[41m[m
[32m+[m[32m        self.request.append(0)[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        self.discriminative()[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    def discriminative(self):[m[41m[m
[32m+[m[32m        sum_ = sum(self.request)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if self.M >= sum_:[m[41m[m
[32m+[m[32m            print(self.request[0])[m[41m[m
[32m+[m[32m        else:[m[41m[m
[32m+[m[32m            for i in range(0, self.N+1):[m[41m[m
[32m+[m[32m                temp = self.request[i:][m[41m[m
[32m+[m[32m                sum_ = sum(temp)[m[41m[m
[32m+[m[32m                sum_ += temp[0]*i[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if sum_ > self.M:[m[41m[m
[32m+[m[32m                    continue[m[41m[m
[32m+[m[32m                else:[m[41m[m
[32m+[m[32m                    if i == 0:[m[41m[m
[32m+[m[32m                        print(self.request[0])[m[41m[m
[32m+[m[32m                    else:[m[41m[m
[32m+[m[32m                        cal = self.M-sum_[m[41m[m
[32m+[m[32m                        result = self.request[i] + (cal//(i))[m[41m[m
[32m+[m[32m                        print(result)[m[41m[m
[32m+[m[32m                        break[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
